#include<stdio.h>
 /*Main() starts*/
 int main()
  {
      int base, height, area;//variables for base, height and area.
      
      printf("Enter value for the base \n");//Messsage displayed to the user
       scanf("%d",&base);//Input
       
      printf("Enter value for the height\n");
       scanf("%d",&height);
       
      area = base * height; //calculating  area
      
      printf("The area of the triangle is: %d",area);  //Output
      return 0;
  } //End of function main

